{
  "dataSources": [
    {
      "id": "main",
      "url": "http://localhost:3000/large_data_set"
    }
  ],
  "pages": [
    {
      "title": "Main Report",
      "navIcon": "home",
      "path": "/",
      "widgets": [
        {
          "id": "summary",
          "type": "summary",
          "data": {
            "dataSourceId": "main"
          },
          "parameters": {
            "title": "My Tests"
          }
        },
        {
          "id": "overview-distribution-pie",
          "type": "distribution-pie",
          "data": {
            "dataSourceId": "main"
          }
        },
        {
          "id": "tests-tree",
          "type": "tree",
          "data": {
            "dataSourceId": "main",
            "filter": {
              "operator": "AND",
              "conditions": [
                {
                  "field": "lastExecutionType",
                  "operator": "!=",
                  "value": "SKIPPED"
                }
              ]
            }
          }
        }
      ]
    },
    {
      "title": "The same Page",
      "path": "/path",
      "navIcon": "category",
      "widgets": [
        {
          "id": "overview-distribution-pie",
          "type": "distribution-pie",
          "data": {
            "dataSourceId": "main"
          }
        }
      ]
    },
    {
      "title": "yet another one",
      "path": "another-one",
      "navIcon": "place",
      "widgets": [
        {
          "id": "tree",
          "type": "tree",
          "data": {
            "dataSourceId": "main"
          }
        }
      ]
    }
  ]
}
