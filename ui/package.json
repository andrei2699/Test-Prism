{
  "name": "test-prism-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "npm test -- --watch=false",
    "prettier:fix": "prettier --write .",
    "json-server": "node __mocks__/server.js"
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "bracketSameLine": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.10.0",
  "dependencies": {
    "@angular/animations": "^21.1.4",
    "@angular/cdk": "^21.1.4",
    "@angular/common": "^21.1.4",
    "@angular/compiler": "^21.1.4",
    "@angular/core": "^21.1.4",
    "@angular/forms": "^21.1.4",
    "@angular/material": "^21.1.4",
    "@angular/platform-browser": "^21.1.4",
    "@angular/router": "^21.1.4",
    "chart.js": "^4.5.1",
    "humanize-duration": "^3.33.2",
    "ng2-charts": "^8.0.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular/build": "^21.1.4",
    "@angular/cli": "^21.1.4",
    "@angular/compiler-cli": "^21.1.4",
    "@types/humanize-duration": "^3.27.4",
    "@vitest/browser-playwright": "^4.0.18",
    "jsdom": "^28.0.0",
    "json-server": "0.17.4",
    "prettier": "^3.8.1",
    "typescript": "~5.9.3",
    "vitest": "^4.0.18"
  }
}
